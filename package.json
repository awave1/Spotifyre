{
  "name": "Spotifyre",
  "version": "1.0.0",
  "repository": "git@github.com:natekchua/Spotifyre.git",
  "scripts": {
    "install:client": "yarn --cwd client install",
    "install:server": "yarn --cwd server install",
    "postinstall": "npm-run-all --parallel setupHooks install:*",
    "start:client": "yarn --cwd client start",
    "start:server": "yarn --cwd server dev",
    "dev": "npm-run-all --parallel start:*",
    "lint:client": "yarn --cwd client lint",
    "lint:server": "yarn --cwd server lint",
    "lint": "npm-run-all --parallel lint:*",
    "setupHooks": "husky install"
  },
  "devDependencies": {
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^5.0.6",
    "npm-run-all": "^4.1.5",
    "prettier-standard": "^16.4.1"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
