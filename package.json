{
  "name": "Spotifyre",
  "version": "1.0.0",
  "repository": "git@github.com:natekchua/Spotifyre.git",
  "scripts": {
    "install:client": "yarn --cwd client install",
    "install:server": "yarn --cwd server install",
    "postinstall": "npm-run-all --parallel install:*",
    "start:client": "yarn --cwd client start",
    "start:server": "yarn --cwd server dev",
    "dev": "npm-run-all --parallel start:*",
    "lint:client": "yarn --cwd client lint",
    "lint:server": "yarn --cwd server lint",
    "lint": "npm-run-all --parallel lint:*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier-standard": "^16.4.1"
  },
  "dependencies": {}
}